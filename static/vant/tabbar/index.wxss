@import '../common/index.wxss';

.van-tabbar {
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 70px;
	background-color: #fff
}

.van-tabbar--fixed {
	position: fixed;
	bottom: 0;
	left: 0;
    background:#f6f7f8;
    border-top-left-radius: 30px;
	border-top-right-radius: 30px;
    color:#999;
}

.van-tabbar--safe {
	padding-bottom: 34px
}